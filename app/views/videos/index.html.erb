<div class="container">
  <h1>Your Vids</h1>
  <table id="tableView">

  <% @videos.each do |video| %>
    <tr>
      <td class="title"><%= video.title %></td>
      <td><%= youtube_video_thumb video.url %></td>
      <td class="tags"><%= video.tags %></td>

      <td><%= link_to 'Show', video %></td>
      <td><%= link_to 'Edit', edit_video_path(video) %></td>
      <td><%= link_to image_tag('delete'), video, method: :delete, data: { confirm: 'You sure about that?' } %></td>
    </tr>
  <% end %>
  </table>

  <br />

  <%= link_to 'New Video', new_video_path %>
</div>